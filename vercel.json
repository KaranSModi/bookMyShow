{
    "version": 2,
    "builds": [
        {
            "src": "core/wsgi.py",
            "use": "@ardnt/vercel-python-wsgi",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.8.10"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "vercel_app/wsgi.py"
        }
    ]
}